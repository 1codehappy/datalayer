{
    "name": "c0dehappy/datalayer",
    "description": "A simplest abstraction for eloquent models",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "repository",
      "design",
      "best practice",
      "simple",
      "abstract",
      "pattern",
      "datalayer",
      "model",
      "laravel",
      "eloquent",
      "decorator"
    ],
    "autoload": {
        "psr-4": {
            "CodeHappy\\DataLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeHappy\\DataLayer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/cache": "~5.6|~6.0",
        "illuminate/console": "~5.6|~6.0",
        "illuminate/database": "~5.6|~6.0",
        "illuminate/pagination": "~5.6|~6.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "extra": {
      "laravel": {
          "providers": [
          ],
          "aliases": {
              "QueryFactory": "CodeHappy\\DataLayer\\Facades\\QueryFactory"
          }
      }
    }
}
